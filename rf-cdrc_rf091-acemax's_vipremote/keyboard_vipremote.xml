<!-- Mappings for CDRC RF091: Acemax's Vipremote -->
<!-- This is a custom buttons layout, I recommend putting stickers on part of the buttons -->
<!-- Custom layout image at -->
<!-- Created by dhead666 -->
<keymap>
  <global>
    <keyboard name="Motorola Nyxboard Hybrid">
      <!-- n/a -->							<!-- Button 01 : Power -->
      <volume_mute>Mute</volume_mute>					<!-- Button 02 : Mute -->
      <r>Rewind</r>							<!-- Button 03 : << -->
      <stop>Stop</stop>							<!-- Button 04 : stop -->
      <play_pause>Play</play_pause>					<!-- Button 05 : > -->
      <prev_track>SkipPrevious</prev_track>				<!-- Button 06 : |<< -->
      <f>FastForward</f>						<!-- Button 07 : >> -->
      <!-- n/a -->							<!-- Button 08 : Record -->
      <pause>Pause</pause> 						<!-- Button 09 : || -->
      <next_track>SkipNext</next_track>					<!-- Button 10 : >>| -->
      <f8>noop</f8> <!-- Mouse On/Off Fix -->				<!-- Button 11 : Mouse Toggle -->
      <!-- n/a -->							<!-- Button 12 : Mouse Left Click -->
      <!-- n/a -->							<!-- Button 13 : Mouse Right Click -->
      <up>Up</up>							<!-- Button 14 : up -->
      <left>Left</left>							<!-- Button 15 : left -->
      <return>Select</return>						<!-- Button 16 : OK -->
      <right>Right</right>						<!-- Button 17 : right -->
      <down>Down</down>							<!-- Button 18 : down -->
      <volume_up>VolumeUp</volume_up>					<!-- Button 19 : Vol+ -->
      <volume_down>VolumeDown</volume_down>				<!-- Button 20 : Vol- -->
      <f2>PageUp</f2>  							<!-- Button 21 : pg+ -->
      <f3>PageDown</f3>							<!-- Button 22 : pg- -->
      <home>ActivateWindow(Home)</home>					<!-- Button 23 : home -->
      <f4>Back</f4>							<!-- Button 24 : back -->
      <one>Number1</one>						<!-- Button 25 : 1 -->
      <two>Number2</two>						<!-- Button 26 : 2 -->
      <three>Number3</three>						<!-- Button 27 : 3 -->
      <four>Number4</four>						<!-- Button 28 : 4 -->
      <five>Number5</five>						<!-- Button 29 : 5 -->
      <six>Number6</six>						<!-- Button 30 : 6 -->
      <seven>Number7</seven>						<!-- Button 31 : 7 -->
      <eight>Number8</eight>						<!-- Button 32 : 8 -->
      <nine>Number9</nine>						<!-- Button 33 : 9 -->
      <zero>Number0</zero>						<!-- Button 34 : 0 -->
      <plus>Info</plus>	<!-- or plus -->				<!-- Button 35 : Info -->
      <numpadminus>Playlist</numpadminus>				<!-- Button 36 : List -->
      <pageup>ActivateWindow(Settings)</pageup>				<!-- Button 37 : Settings -->
      <pagedown>ContextMenu</pagedown>					<!-- Button 38 : Menu -->
      <numpadnine>FullScreen</numpadnine>				<!-- Button 39 : */# -->
      <numpadzero>noop</numpadzero>					<!-- Button 40 : FAV -->
      <f9>Close</f9>  							<!-- Button 41 : Red -->
      <f10>Close</f10>							<!-- Button 42 : Green -->
      <f11>Close</f11>							<!-- Button 43 : Yellow -->
      <f12>Close</f12>							<!-- Button 44 : Blue -->
    </keyboard>
  </global>
